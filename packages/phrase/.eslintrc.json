{
  "env": {
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["typescript", "plugin:jest/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "plugins": ["jest"],
  "ignorePatterns": ["example.js"],
  "rules": {
    "jest/no-disabled-tests": "off"
  }
}
